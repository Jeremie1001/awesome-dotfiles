{
  "extends": "steelbrain",
  "rules": {
    "no-underscore-dangle": "off",
    "import/prefer-default-export": "off",
    "import/no-unresolved": ["error", {
      "ignore": ["atom"]
    }]
  },
  "globals": {
    "atom": true,
    "BufferedProcess": true,
    "__steelbrain_package_deps": true
  }
}
